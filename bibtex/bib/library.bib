keywords = {Clutter,Coherence,Humans,IPDA,ROC,SNR,Signal to noise ratio,Silicon,Target tracking,human diver detection,human diver tracking method,integrated probabilistic data association,moment-based target power estimator,receiving operating characteristic,sensor fusion,signal-to-noise ratio,sonar detection,target tracking,underwater acoustics},
