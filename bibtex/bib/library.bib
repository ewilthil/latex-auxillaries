title = {{The Autonomous Maritime Navigation (AMN) project: Field tests, autonomous and cooperative behaviors, data fusion, sensors, and vehicles}},
url = {http://doi.wiley.com/10.1002/rob.20367},
volume = {27},
year = {2010}
