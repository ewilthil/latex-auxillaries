journal = {2004 43rd IEEE Conference on Decision and Control (CDC) (IEEE Cat. No.04CH37601)},
keywords = {GNC,UAV},
title = {{An overview of emerging results in cooperative UAV control}},
volume = {1},
year = {2004}
}
