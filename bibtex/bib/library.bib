journal = {International Journal of Navigation and Observation},
keywords = {inertial navigation,kalman filters,sensor systems,state estimation},
pages = {1--6},
title = {{An intuitive approach to inertial sensor bias estimation}},
year = {2013}
}
@article{Voos2009,
abstract = {Quadrotor UAVs are one of the most preferred type of small unmanned aerial vehicles$\backslash$nbecause of the very simple mechanical construction and propulsion principle. However, the$\backslash$nnonlinear dynamic behavior requires a more advanced stabilizing control and guidance of these$\backslash$nvehicles. In addition, the small payload reduces the amount of batteries that can be carried and$\backslash$nthus also limits the operating range of the UAV. One possible solution for a range extension$\backslash$nis the application of a base station for recharging purpose even during operation. In order to$\backslash$nincrease the efficiency of the overall system further, a mobile base station will be applied here.$\backslash$nHowever, landing on a moving base station requires autonomous tracking and landing control$\backslash$nof the UAV. In this paper, a novel nonlinear autopilot for quadrotor UAVs is extended with a$\backslash$ntracking and landing controller to fulfill the required task. First simulation and experimental$\backslash$nresults underline the performance of this new control approach for the current realization.},
author = {Voos, H and Nourghassemi, B},
file = {:Users/ewilthil/Documents/OneDriveBusiness/Papers/Nonlinear Control of Stabilized Flight and Landing for Quadrotor UAVs - Voos, Nourghassemi - 2009.pdf:pdf},
